{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "http://liestudio/schemas/similarity_response.json",
  "title": "Chemical similarity output",
  "description": "Calculate the chemical similarity between structures",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "description": "Job final status",
      "enum": [
        "failed",
        "completed"
      ]
    },
    "results": {
      "type": "object",
      "description": "Statistics object"
    }
  },
  "required": [
    "status",
    "results"
  ]
} 
