{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "http://mdstudio/schemas/endpoints/toolkits_response.v1.json",
  "title": "Chemical similarity input",
  "description": "Check what bioinformatics package are supported by the service",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "description": "Job final status",
      "enum": [
        "failed",
        "completed"
      ]
    },
    "toolkits": {
      "type": "array",
      "description": "Available bioinformatic package"
    }
  },
  "required": [
    "status",
    "toolkits"
  ]
}
