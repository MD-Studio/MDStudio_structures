{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://mdstudio/schemas/info_request.json",
  "description": "Molecular attributes",
  "type": "object",
  "properties": {
    "toolkit": {
      "type": "string",
      "description": "Default cheminformatics toolkit to use",
      "default": "pybel"
    },
    "mol": {
      "$ref": "resource://mdgroup/lie_structures/path_file/v1",
      "description": "Molecular geometry"
    },
    "input_format": {
      "type": "string",
      "description": "Structure input file format"
    },
    "workdir": {
      "type": "string",
      "default": "."
    }
  },
  "required": [
    "mol"
  ]
}
