{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "http://mdstudio/schemas/endpoints/descriptors_response.v1.json",
  "title": "Molecular descriptors output",
  "description": "Molecular descriptors configuration",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "description": "Job final status",
      "enum": [
        "failed",
        "completed"
      ]
    },
    "descriptors": {
      "type": "string",
      "description": "Molecular toolkit use",
      "default": "pybel"
    }
  },
  "required": [
    "status",
    "descriptors"
  ]
}
