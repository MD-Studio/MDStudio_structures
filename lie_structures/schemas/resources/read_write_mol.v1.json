{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://liestudio/schemas/read_write_mol.json",
    "title": "input output config",
    "description": "Config to read and write a mol",
    "type": "object",
    "properties": {
	"toolkit": {
	    "type": "string",
	"description": "Default cheminformatics toolkit to use",
	    "default": "pybel"
	},
	"mol": {
	    "$ref": "resource://mdgroup/common_resources/path_file/v1",
	    "description": "Molecular representation"
	},
	"output_format": {
	    "type": "string",
	    "description": "Structure output file format"
	},
	"workdir": {
	    "type": "string",
	    "default": "."
      }
    },
    "required": [
	"mol"
    ]
}
